<%= button_to 'Logout', destroy_user_session_path, method: :delete %>
<h2>Welcome <%= current_user.full_name %></h2>
<hr/>
<%= link_to 'Add Account', new_account_path %>
<h3>Your Accounts</h3>
<ul>
  <% @accounts.each do |a| %>
    <li>
      <%= a.name %> - $<%= a.balance%>
    </li>
  <% end %>
</ul>